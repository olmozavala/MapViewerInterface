<!-- By default the layers have the following configuration:
width = 512
height = 512

proj = EPSG:4326

time = false

tiled = true
tileOrigLong = -180
tileOrigLat = -90

featureInfo = name
-->

<!-- The width and height of the image affects the resolution of the animated gif it is important, by default is 512 512 -->
<!-- FeatureInfo by default is the same layer name, if it doesn't have data then use 'none' --> 
<!-- Digioia, Giusj  -->
<MenuConf xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="schema.xsd">
	<MainLayers BBOX="-98.0,18.09164810180664,-76.400024,31.960647583007812" 
				format="image/gif"  server="http://viewer.coaps.fsu.edu/ncWMS/wms"
				tiled="true" time="true" mincolor="10" maxcolor="34">  
		<layer Menu="Temp,2010" EN="2010 Temperature" ES="Temperatura del 2010"
								name="hycom2010/temperature"   />
		<layer Menu="Temp,2011" EN="2011 Temperature" ES="Temperatura del 2011"
								name="hycom2011/temperature"   />
		<layer Menu="Temp,2012" EN="2012 Temperature" ES="Temperatura del 2012"
								name="hycom2012/temperature"   />
	</MainLayers>
</MenuConf>